  PAGES = {3011-3053},
  YEAR = {2020},
  MONTH = Jul,
  DOI = {10.5194/gmd-13-3011-2020},
  PDF = {https://hal.archives-ouvertes.fr/hal-02893945/file/gmd-13-3011-2020.pdf},
  HAL_ID = {hal-02893945},
  HAL_VERSION = {v1},
  selected={true},
  bibtex_show={true},
  preview={CM5A2.png}
}

